'global':
  'minimap':
    'plugins':
      'selection': true
      'git-diff': true
  'welcome':
    'showOnStartup': false
  'core':
    'themes': [
      'atom-soda-dark-ui'
      'monokai-soda'
    ]
  'linter-flake8':
    'maxLineLength': 120
  'exception-reporting':
    'userId': '772afc40-7579-582e-4c2f-ff65dd824377'
  'editor':
    'tabLength': 4
    'normalizeIndentOnPaste': false
    'showInvisibles': true
    'showIndentGuide': true
    'invisibles': {}
  'spell-check':
    'grammars': [
      'source.gfm'
      'text.plain.null-grammar'
      'source.c++'
      'source.litcoffee'
      'source.css'
      'text.git-commit'
      'source.go'
      'text.html.basic'
      'source.java'
      'source.js'
      'source.json'
      'source.css.less'
      'source.makefile'
      'source.plist'
      'source.python'
      'text.html.erb'
      'source.shell'
      'source.sql'
      'text.plain'
      'text.todo'
      'source.yaml'
    ]
